Cell["Let's perform the integration: ", "Text",ExpressionUUID->"67e017d7-ca53-6e4e-952c-e27c73b8fe45"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
    "Define", " ", "the", " ", "corrected", " ", "integrand", " ", "with", 
     " ", "expansion", " ", "for", " ", "p"}], " ", ">>", " ", "m"}], " ", 
   "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"integrand", "[", 
      RowBox[{"p_", ",", "m_", ",", "T_", ",", "\[Lambda]_"}], "]"}], ":=", 
     "\[IndentingNewLine]", "   ", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{"(", 
        RowBox[{"1", "/", 
         RowBox[{"(", 
          RowBox[{"3", "T"}], ")"}]}], ")"}]}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"p", "^", "3"}], 
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{
           RowBox[{"m", "^", "2"}], "/", 
           RowBox[{"(", 
            RowBox[{"2", 
             RowBox[{"p", "^", "2"}]}], ")"}]}]}], ")"}]}], ")"}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"1", "/", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"Exp", "[", 
             RowBox[{"p", "/", "T"}], "]"}], 
            RowBox[{"\[Lambda]", "^", 
             RowBox[{"(", 
              RowBox[{"-", "1"}], ")"}]}]}], "+", "1"}], ")"}]}], "-", 
        "\[IndentingNewLine]", "     ", 
        RowBox[{
         RowBox[{
          RowBox[{"m", "^", "2"}], "/", 
          RowBox[{"(", 
           RowBox[{"2", "p", "T"}], ")"}]}], 
         RowBox[{"Exp", "[", 
          RowBox[{"p", "/", "T"}], "]"}], 
         RowBox[{
          RowBox[{"\[Lambda]", "^", 
           RowBox[{"(", 
            RowBox[{"-", "1"}], ")"}]}], "/", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"Exp", "[", 
               RowBox[{"p", "/", "T"}], "]"}], 
              RowBox[{"\[Lambda]", "^", 
               RowBox[{"(", 
                RowBox[{"-", "1"}], ")"}]}]}], "+", "1"}], ")"}], "^", 
           "2"}]}]}]}], ")"}]}]}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Perform", " ", "the", " ", "integration"}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{"integrand", "[", 
      RowBox[{"p", ",", "m", ",", "T", ",", "\[Lambda]"}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"p", ",", "0", ",", "Infinity"}], "}"}]}], "]"}]}]}]], "Input",E\
xpressionUUID->"a53b8646-2329-ad41-ad74-dafe4c37da3b"],

