      RowBox[{"T", "*", 
       RowBox[{"Log", "[", "\[Lambda]", "]"}]}], ",", "T"}], "]"}], ";"}], 
   "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"massiveIntegrandRelativisticSeriesList", "[", 
      RowBox[{"p_", ",", "m_", ",", "\[Mu]_", ",", "T_"}], "]"}], " ", ":=", 
     " ", 
     RowBox[{"List", "@@", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"massiveIntegrandRelativisticSeries", "[", 
         RowBox[{"p", ",", "m", ",", "\[Mu]", ",", "T"}], "]"}], "//", 
        "Expand"}], ")"}]}]}], ";"}], "\n", 
   RowBox[{"massiveIntegrandRelativisticSeriesList", "[", 
    RowBox[{"p", ",", "m", ",", "\[Mu]", ",", "T"}], "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Series", " ", "expansion", " ", "WITHOUT", " ", "massless", " ", 
