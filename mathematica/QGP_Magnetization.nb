Cell["Evaluating Fermi Integrals for various limits", "Section",ExpressionUUID->"9cd3aa01-c37d-dd4a-9ea4-91c52456a53a"],

Cell["\<\
We're going to define a set of expressions which allow us to evaluate the \
Fermi integral for various important limits. To start, we define the Fermi \
Partition function which we need to integrate over to obtain the free energy. \
Below we write the fermion partition function and the coefficient for the \
free energy.\
\>", "Text",
 CellChangeTimes->{{3.938848058949095*^9, 3.9388480599269924`*^9}, {
  3.938852859713167*^9, 
  3.938852884380827*^9}},ExpressionUUID->"4c143761-c233-574e-9a16-\
b64d5b3a1546"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"Define", " ", "variables", " ", "and", " ", "conditions"}], " ", 
   "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"chemicalPotentials", "=", 
     RowBox[{"{", 
      RowBox[{"\[Mu]", ",", 
       RowBox[{"-", "\[Mu]"}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"thermodynamicVariables", "=", 
     RowBox[{"{", 
      RowBox[{"T", ",", "m", ",", "\[Mu]", ",", "p", ",", "\[Lambda]"}], 
      "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"allThermodynamicVariablesPositive", "=", 
     RowBox[{"Positive", "/@", "thermodynamicVariables"}]}], ";"}], "\n", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Define", " ", "particle", " ", "energy", " ", "and", " ", "partition", 
     " ", "function"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"particleEnergy", "[", 
      RowBox[{"p_", ",", "m_"}], "]"}], ":=", 
     RowBox[{"Sqrt", "[", 
      RowBox[{
       RowBox[{"m", "^", "2"}], "+", 
       RowBox[{"p", "^", "2"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"fermiPartition", "[", 
      RowBox[{"p_", ",", "m_", ",", "\[Mu]_", ",", "T_"}], "]"}], ":=", 
     RowBox[{"Log", "[", 
      RowBox[{"1", "+", 
       RowBox[{
        RowBox[{"Exp", "[", 
         RowBox[{"\[Mu]", "/", "T"}], "]"}], 
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", 
           RowBox[{"particleEnergy", "[", 
            RowBox[{"p", ",", "m"}], "]"}]}], "/", "T"}], "]"}]}]}], "]"}]}], 
    ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"freeEnergyCoeff", "[", 
      RowBox[{"T_", ",", "V_", ",", "N_"}], "]"}], ":=", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{"(", 
        RowBox[{"4", "\[Pi]", " ", "T", " ", "V", " ", "N"}], ")"}]}], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"2", "\[Pi]"}], ")"}], "^", 
       RowBox[{"(", 
        RowBox[{"-", "3"}], ")"}]}]}]}], ";"}]}]}]], "Code",
