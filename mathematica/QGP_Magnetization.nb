Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Define", " ", "the", " ", "relativistic", " ", "limit", " ", "for", " ", 
    "the", " ", "energy", " ", "eigenstate"}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"massOrderExpansion", " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{"m", ",", "0", ",", "4"}], "}"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"particleEnergyRelativisticSeries", "[", 
      RowBox[{"p_", ",", "m_"}], "]"}], ":=", "\[IndentingNewLine]", "\t", 
     RowBox[{
      RowBox[{"Normal", "[", 
       RowBox[{"Series", "[", 
        RowBox[{
         RowBox[{"Sqrt", "[", 
          RowBox[{
           RowBox[{"m", "^", "2"}], "+", 
           RowBox[{"p", "^", "2"}]}], "]"}], ",", "massOrderExpansion"}], 
        "]"}], "]"}], "//", "\n", "\t\t", 
      RowBox[{
       RowBox[{"Refine", "[", 
        RowBox[{"#", ",", "allThermodynamicVariablesPositive"}], "]"}], 
       "&"}]}]}], ";"}], "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Display", " ", "the", " ", "expansion", " ", "of", " ", "the", " ", 
     "microstate", " ", "energies"}], " ", "*)"}], "\n", 
   RowBox[{"particleEnergyRelativisticSeries", "[", 
    RowBox[{"p", ",", "m"}], "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Define", " ", "the", " ", "massive", " ", "correction", " ", "with", " ",
      "integration", " ", "by", " ", "parts"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"massiveIntegrand", "[", 
      RowBox[{"p_", ",", "m_", ",", "\[Mu]_", ",", "T_"}], "]"}], ":=", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{"(", 
        RowBox[{"1", "/", "3"}], ")"}]}], 
      RowBox[{"p", "^", 
       RowBox[{"(", "3", ")"}]}], "*", "\n", "\t", 
      RowBox[{"D", "[", 
       RowBox[{
        RowBox[{"fermiPartition", "[", 
         RowBox[{"p", ",", "m", ",", "\[Mu]", ",", "T"}], "]"}], ",", "p"}], 
       "]"}]}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"massiveIntegrandCorrections", "[", 
      RowBox[{"p_", ",", "m_", ",", "\[Mu]_", ",", "T_"}], "]"}], ":=", 
